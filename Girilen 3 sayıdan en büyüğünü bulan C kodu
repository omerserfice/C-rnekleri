#include <stdio.h>
#include <stdlib.h>

int main()
{


     int i,enb=0,j;
     int sayi[3];

     for(i=0;i<3;i++){

        printf("%d.sayiyi giriniz:",i+1);
        scanf("%d",&sayi[i]);

        if(enb<sayi[i])

            enb=sayi[i];


     }
     printf("En buyuk sayi:%d",enb);


    return 0;
}
